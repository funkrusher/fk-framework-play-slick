# server endpoints
GET   /                           server.controllers.HealthcheckController.healtcheck()

# modules
->    /api/library                library.Routes
->    /api/store                  store.Routes

# swagger-ui
GET /swagger.json                 controllers.Assets.at(path="/public", file="swagger.json")
GET /docs/*file                   controllers.Assets.at(path="/swagger-ui", file)

# static assets
GET /assets/*file                 controllers.Assets.versioned(path="/public", file: Asset)
